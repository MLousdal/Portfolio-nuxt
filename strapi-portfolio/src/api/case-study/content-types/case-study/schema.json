{
  "kind": "collectionType",
  "collectionName": "case_studies",
  "info": {
    "singularName": "case-study",
    "pluralName": "case-studies",
    "displayName": "Case-study",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Name": {
      "type": "string"
    },
    "MainImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "Mockups": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "Description": {
      "type": "richtext",
      "default": "### beskrivelse"
    },
    "tools": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tool.tool",
      "inversedBy": "case_studies"
    },
    "projectLinks": {
      "type": "component",
      "repeatable": true,
      "component": "links.project-links"
    }
  }
}
